/****** Script for SelectTopNRows command from SSMS  ******/
SELECT TOP 1000 [MED_REC_ID]
      ,[ORGAN_CODE]
      ,[ORGAN_NAME]
      ,[PAY_MODE]
      ,[HP_NUMB]
      ,[HP_S_NO]
      ,[MR_NO]
      ,[CARDTYPE]
      ,[CARDNO]
      ,[CER_UNK_REA]
      ,[OTH_REA_EXP]
      ,[NAME]
      ,[SEX]
      ,[BIRTHDAY]
      ,[AGE_YEAR]
      ,[AGE_MONTH]
      ,[AGE_DAY]
      ,[NATION]
      ,[B_WEIGHT_1]
      ,[B_WEIGHT_2]
      ,[IN_HP_WEIGHT]
      ,[BIRTH_ADDR]
      ,[BIRTH_ADDR_1]
      ,[BIRTH_ADDR_2]
      ,[ORYGIN_ADDR]
      ,[ORYGIN_ADDR_1]
      ,[NATION_CODE]
      ,[JOB_CODE]
      ,[MARR_CODE]
      ,[CUR_ADDR_CODE]
      ,[CUR_ADDR]
      ,[CUR_ADDR_1]
      ,[CUR_ADDR_2]
      ,[CUR_ADDR_3]
      ,[CUR_ADDR_4]
      ,[CUR_ADDR_5]
      ,[PAT_PHONE]
      ,[CUR_POSTCODE]
      ,[HR_ADDR_CODE]
      ,[HR_ADDR]
      ,[HR_ADDR_1]
      ,[HR_ADDR_2]
      ,[HR_ADDR_3]
      ,[HR_ADDR_4]
      ,[HR_ADDR_5]
      ,[HR_POSTCODE]
      ,[WORK_ORGAN]
      ,[ORGAN_PHONE]
      ,[ORGAN_POSTCODE]
      ,[CON_NAME]
      ,[CON_PAT_CODE]
      ,[CON_ADDR]
      ,[CON_PHONE]
      ,[IN_HP_WAY_CODE]
      ,[IN_HP_DT]
      ,[IN_DPT_CODE]
      ,[IN_DPT_NAME]
      ,[IN_HP_WARD]
      ,[TRANS_CAT]
      ,[OUT_HP_DT]
      ,[OUT_DPT_CODE]
      ,[OUT_DPT_NAME]
      ,[OUT_HP_WARD]
      ,[H_O_D]
      ,[OE_DIS_NAME]
      ,[OE_DIS_CODE]
      ,[HM_DIS_CODE]
      ,[HM_DIS_NAME]
      ,[M_DIS_CODE]
      ,[M_DIS_NAME]
      ,[M_DIS_COND]
      ,[TRABSFERRED]
      ,[M_DIS_DES]
      ,[M_DIS_CODE_1]
      ,[M_DIS_NAME_1]
      ,[M_DIS_COND_1]
      ,[TRABSFERRED_1]
      ,[M_DIS_CODE_2]
      ,[M_DIS_NAME_2]
      ,[M_DIS_COND_2]
      ,[TRABSFERRED_2]
      ,[M_DIS_CODE_3]
      ,[M_DIS_NAME_3]
      ,[M_DIS_COND_3]
      ,[TRABSFERRED_3]
      ,[M_DIS_CODE_4]
      ,[M_DIS_NAME_4]
      ,[M_DIS_COND_4]
      ,[TRABSFERRED_4]
      ,[M_DIS_CODE_5]
      ,[M_DIS_NAME_5]
      ,[M_DIS_COND_5]
      ,[TRABSFERRED_5]
      ,[M_DIS_CODE_6]
      ,[M_DIS_NAME_6]
      ,[M_DIS_COND_6]
      ,[TRABSFERRED_6]
      ,[M_DIS_CODE_7]
      ,[M_DIS_NAME_7]
      ,[M_DIS_COND_7]
      ,[TRABSFERRED_7]
      ,[M_DIS_CODE_8]
      ,[M_DIS_NAME_8]
      ,[M_DIS_COND_8]
      ,[TRABSFERRED_8]
      ,[M_DIS_CODE_9]
      ,[M_DIS_NAME_9]
      ,[M_DIS_COND_9]
      ,[TRABSFERRED_9]
      ,[EXTL_CAUSE]
      ,[EXTL_CAUSE_CODE]
      ,[PATHO_DIAG]
      ,[PATHO_DIAG_CODE]
      ,[PATHOEX_NO]
      ,[ALLERGY_MARK]
      ,[ALDRUG_NAME]
      ,[AUTOPSY]
      ,[ABO_CODE]
      ,[RH_CODE]
      ,[SCHEAD_NAME]
      ,[CPHY_NAME]
      ,[RSPHY_IDCARD]
      ,[RSPHY_NAME]
      ,[HPHY_NAME]
      ,[PRIMARY_NUR]
      ,[RFRPHY_NAME]
      ,[INTERN_NAME]
      ,[CODER_NAME]
      ,[MRQU_CODE]
      ,[QUCTRDOC_NAME]
      ,[QUCTRNURSE]
      ,[QUCTR_DATE]
      ,[IF_OPREA]
      ,[REC_NUMB]
      ,[DISCHAWAYCODE]
      ,[INTORGAN_NAME]
      ,[IF_31RHP]
      ,[RHP_P]
      ,[PAT_IH_FD]
      ,[PAT_IH_FH]
      ,[PAT_IH_FM]
      ,[PAT_IH_BD]
      ,[PAT_IH_BH]
      ,[PAT_IH_BM]
      ,[TOTAL_COST]
      ,[IND_PAY]
      ,[GEN_MED_SF]
      ,[GEN_CUR_SF]
      ,[NURSING_FEE]
      ,[OTH_TRE_SF]
      ,[PATH_DIAG_FEE]
      ,[LAB_DIAG_FEE]
      ,[IMAG_DIAG_FEE]
      ,[CLIN_DIAG_FEE]
      ,[NON_OP_T_FEE]
      ,[NON_OP_TCP_FEE]
      ,[OP_T_FEE]
      ,[ANES_FEE]
      ,[OP_FEE]
      ,[REHAB_FEE]
      ,[CM_TREAT_FEE]
      ,[W_MED_FEE]
      ,[ANT_COST_FEE]
      ,[C_MED_FEE]
      ,[CH_MED_FEE]
      ,[BLOOD_FEE]
      ,[ALBUMIN_FEE]
      ,[GLOBULIN_FEE]
      ,[COAG_FACT_FEE]
      ,[COLL_FACT_FEE]
      ,[CH_USE_DMM_FEE]
      ,[TR_USE_DMM_FEE]
      ,[OP_USE_DMM_FEE]
      ,[OTHER_FEE]
      ,[ICU_CODE_1]
      ,[ICU_NAME_1]
      ,[IN_ICU_DATE_1]
      ,[OUT_ICU_DATE_1]
      ,[ICU_CODE_2]
      ,[ICU_NAME_2]
      ,[IN_ICU_DATE_2]
      ,[OUT_ICU_DATE_2]
      ,[ICU_CODE_3]
      ,[ICU_NAME_3]
      ,[IN_ICU_DATE_3]
      ,[OUT_ICU_DATE_3]
      ,[IF_CRI_CASE]
      ,[IF_DIF_CASE]
      ,[IF_INF_CASE]
      ,[IF_UN_PNEU]
      ,[IF_MDT_CASE]
      ,[IF_SD_CASE]
      ,[IF_DO_CASE]
      ,[IF_TEACH_MR]
      ,[OPHPDIAG_CI]
      ,[ADDIAG_CI]
      ,[OPERDIAG_CI]
      ,[CCASEDIAG_CI]
      ,[RCASEDIAG_CI]
      ,[RSC_NO]
      ,[SC_RSC_NO]
      ,[CPW_MANA_CODE]
      ,[REC_IDCARD]
      ,[REC_NAME]
      ,[RCD_DT]
      ,[POF_DT]
  FROM [shengPingTai].[spt].[V_FirstPageMedicalRecord_2015]
  where AGE_DAY like '-'
  and AGE_MONTH  like '-'
  and AGE_YEAR  like '-'
  and TOTAL_COST  like '-'
  and IND_PAY  like '-'
  and GEN_CUR_SF  like '-'
  and GEN_MED_SF  like '-'
  and NURSING_FEE  like '-'
  and OTH_TRE_SF  like '-'
  and PATH_DIAG_FEE  like '-'
  and LAB_DIAG_FEE  like '-'
  and IMAG_DIAG_FEE like '-'
  and CLIN_DIAG_FEE  like '-'
  and NON_OP_T_FEE like '-'
  and NON_OP_TCP_FEE like '-'
  and OP_T_FEE like '-'
  and ANES_FEE like '-'
  and OP_FEE like '-'
  and REHAB_FEE like '-'
  and CM_TREAT_FEE like '-'
  and W_MED_FEE like '-'
  and ANT_COST_FEE like '-'
  and C_MED_FEE  like '-'
  and CH_MED_FEE like '-'
  and BLOOD_FEE like '-'
  and ALBUMIN_FEE like '-'
  and GLOBULIN_FEE like '-'
  and COAG_FACT_FEE  like '-'
  and COLL_FACT_FEE like '-'
  and CH_USE_DMM_FEE like '-'
  and TR_USE_DMM_FEE like '-'
  and OP_USE_DMM_FEE like '-'
  and OTHER_FEE like '-'
   
=================================================================

where  TOTAL_COST     like '%-%'
or IND_PAY        like '%-%'
or GEN_MED_SF     like '%-%'
or GEN_CUR_SF     like '%-%'
or NURSING_FEE    like '%-%'
or OTH_TRE_SF     like '%-%'
or PATH_DIAG_FEE  like '%-%'
or LAB_DIAG_FEE   like '%-%'
or IMAG_DIAG_FEE  like '%-%'
or CLIN_DIAG_FEE  like '%-%'
or NON_OP_T_FEE   like '%-%'
or NON_OP_TCP_FEE like '%-%'
or OP_T_FEE       like '%-%'
or ANES_FEE       like '%-%'
or OP_FEE         like '%-%'
or REHAB_FEE      like '%-%'
or CM_TREAT_FEE   like '%-%'
or W_MED_FEE      like '%-%'
or ANT_COST_FEE   like '%-%'
or C_MED_FEE      like '%-%'
or CH_MED_FEE     like '%-%'
or BLOOD_FEE      like '%-%'
or ALBUMIN_FEE    like '%-%'
or GLOBULIN_FEE   like '%-%'
or COAG_FACT_FEE  like '%-%'
or COLL_FACT_FEE  like '%-%'
or CH_USE_DMM_FEE like '%-%'
or TR_USE_DMM_FEE like '%-%'
or OP_USE_DMM_FEE like '%-%'
or OTHER_FEE      like '%-%';
